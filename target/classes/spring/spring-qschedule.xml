<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:qschedule="http://www.qunar.com/schema/qschedule"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.qunar.com/schema/qschedule
        http://www.qunar.com/schema/qschedule/qschedule.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">

    <!-- 自动下架过期竞拍物品 -->
    <qschedule:config port="20088"/>
    <context:component-scan base-package="com.qunar.qauction.job"/>


</beans>